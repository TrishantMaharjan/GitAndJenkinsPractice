pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        sh 'echo passed'
        git branch: 'main', url: 'https://github.com/TrishantMaharjan/GitAndJenkinsPractice.git'
      }
    }
    stage('Build') {
      steps {
        sh 'JavaPractice/javac Area.java'
      }
    }
    stage('Test') {
      steps {
        sh 'JavaPractice/java Area'
      }
    }
  }
}

